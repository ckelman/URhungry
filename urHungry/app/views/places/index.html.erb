<h1>Listing places</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Rating</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @places.each do |place| %>
      <tr>
        <td><%= place.name %></td>
        <td><%= place.description %></td>
        <td><%= place.rating %></td>
        <td><%= link_to 'Show', place %></td>

    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Place', new_place_path %>
